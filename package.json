    "chokidar-cli": "^3.0.0",
    "commander": "^12.0.0",
    "globby": "^14.0.1",
    "handlebars": "^4.7.8",
    "linebyline": "^1.3.0",
    "live-server": "^1.2.2",
    "mjml": "^4.15.3",
