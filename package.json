{
  "name": "Email MJML Template v2",
  "version": "1.0.0",
  "description": "Examples",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "npm run build && npm run watch",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run serve\"",
